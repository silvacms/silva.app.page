<?xml version="1.0" encoding="utf-8"?>
<silva xmlns="http://infrae.com/namespace/silva"
	xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:silva-app-document="http://infrae.com/namespace/silva-app-document"
    xmlns:silva-app-page="http://infrae.com/namespace/silva-app-page"
	xmlns:silva-app-news="http://infrae.com/namespace/silva-app-news"
	xmlns:silva-content="http://infrae.com/namespace/metadata/silva-content"
	xmlns:silva-core-contentlayout="http://infrae.com/namespace/silva-core-contentlayout"
	xmlns:silva-core-editor="http://infrae.com/namespace/silva-core-editor"
	xmlns:silva-external-sources="http://infrae.com/namespace/silva-external-sources"
	xmlns:silva-extra="http://infrae.com/namespace/metadata/silva-extra"
	silva_version="Silva 3.0dev">
	<silva-app-news:news_publication id="news">
		<metadata>
			<set id="silva-content">
				<silva-content:maintitle>News</silva-content:maintitle>
				<silva-content:shorttitle />
			</set>
			<set id="silva-extra">
				<silva-extra:comment />
				<silva-extra:contactemail />
				<silva-extra:contactname />
				<silva-extra:content_description />
				<silva-extra:creationtime>2012-05-25T12:45:36Z</silva-extra:creationtime>
				<silva-extra:creator>editor</silva-extra:creator>
				<silva-extra:hide_from_tocs>hide</silva-extra:hide_from_tocs>
				<silva-extra:keywords />
				<silva-extra:language />
				<silva-extra:lastauthor>editor</silva-extra:lastauthor>
				<silva-extra:modificationtime>2012-05-25T12:45:36Z</silva-extra:modificationtime>
				<silva-extra:subject />
			</set>
			<set xmlns:snn-np-settings="http://infrae.com/namespace/metadata/snn-np-settings"
				id="snn-np-settings">
				<snn-np-settings:is_private>no</snn-np-settings:is_private>
			</set>
		</metadata>
		<content>
			<default>
				<silva-app-news:news_viewer id="index"
					number_is_days="0" number_to_show="25" number_to_show_archive="10">
					<metadata>
						<set id="silva-content">
							<silva-content:maintitle>News</silva-content:maintitle>
							<silva-content:shorttitle />
						</set>
						<set id="silva-extra">
							<silva-extra:comment />
							<silva-extra:contactemail />
							<silva-extra:contactname />
							<silva-extra:content_description />
							<silva-extra:creationtime>2012-05-25T12:45:36Z</silva-extra:creationtime>
							<silva-extra:creator>editor</silva-extra:creator>
							<silva-extra:hide_from_tocs>do not hide</silva-extra:hide_from_tocs>
							<silva-extra:keywords />
							<silva-extra:language />
							<silva-extra:lastauthor>editor</silva-extra:lastauthor>
							<silva-extra:modificationtime>2012-05-25T12:45:36Z</silva-extra:modificationtime>
							<silva-extra:subject />
						</set>
					</metadata>
					<content>
						<silva-app-news:filters>
							<silva-app-news:filter target="news/filter" />
						</silva-app-news:filters>
					</content>
				</silva-app-news:news_viewer>
			</default>
			<silva-app-page:agenda_page id="agendapage">
				<workflow>
					<version id="0">
						<status>unapproved</status>
						<publication_datetime />
						<expiration_datetime />
					</version>
				</workflow>
				<content display_datetime="" subjects="root"
					target_audiences="root" version_id="0">
					<metadata>
						<set id="silva-content">
							<silva-content:maintitle>An Agenda Page</silva-content:maintitle>
							<silva-content:shorttitle />
						</set>
						<set id="silva-extra">
							<silva-extra:comment />
							<silva-extra:contactemail />
							<silva-extra:contactname />
							<silva-extra:content_description />
							<silva-extra:creationtime>2012-05-25T12:45:36Z</silva-extra:creationtime>
							<silva-extra:creator>editor</silva-extra:creator>
							<silva-extra:hide_from_tocs>do not hide</silva-extra:hide_from_tocs>
							<silva-extra:keywords />
							<silva-extra:language />
							<silva-extra:lastauthor>editor</silva-extra:lastauthor>
							<silva-extra:modificationtime>2012-05-25T12:45:36Z</silva-extra:modificationtime>
							<silva-extra:subject />
						</set>
					</metadata>
					<silva-core-contentlayout:design id="adesign">
						<silva-core-contentlayout:slot id="one">
							<silva-app-page:agendainfoblock />
						</silva-core-contentlayout:slot>
					</silva-core-contentlayout:design>
				</content>
			</silva-app-page:agenda_page>
			<silva-app-news:news_filter id="filter"
				show_agenda_items="True" subjects="generic,root" target_audiences="all,root">
				<metadata>
					<set id="silva-content">
						<silva-content:maintitle>Filter for News</silva-content:maintitle>
						<silva-content:shorttitle />
					</set>
					<set id="silva-extra">
						<silva-extra:comment />
						<silva-extra:contactemail />
						<silva-extra:contactname />
						<silva-extra:content_description />
						<silva-extra:creationtime>2012-05-25T12:45:36Z</silva-extra:creationtime>
						<silva-extra:creator>editor</silva-extra:creator>
						<silva-extra:hide_from_tocs>do not hide</silva-extra:hide_from_tocs>
						<silva-extra:keywords />
						<silva-extra:language />
						<silva-extra:lastauthor>editor</silva-extra:lastauthor>
						<silva-extra:modificationtime>2012-05-25T12:45:36Z</silva-extra:modificationtime>
						<silva-extra:subject />
					</set>
				</metadata>
				<content>
					<silva-app-news:sources>
						<silva-app-news:source target="news" />
					</silva-app-news:sources>
					<silva-app-news:excludes />
				</content>
			</silva-app-news:news_filter>
		</content>
	</silva-app-news:news_publication>
</silva>
